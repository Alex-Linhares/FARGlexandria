<?xml version="1.0" encoding="utf-8"?>
<namespace name="ObserverUnit">
  <interface name="IObserverActivation">
    <procedure name="Update" visibility="public" procflags="abstract virtual">
      <parameters>
        <parameter name="A" type="Real" />
      </parameters>
    </procedure>
  </interface>
  <interface name="ISubjectActivation">
    <procedure name="RegisterObserver" visibility="public" procflags="abstract virtual">
      <parameters>
        <parameter name="Observer" type="TObject" paramflags="const" />
      </parameters>
    </procedure>
    <procedure name="UnRegisterObserver" visibility="public" procflags="abstract virtual">
      <parameters>
        <parameter name="Observer" type="TObject" paramflags="const" />
      </parameters>
    </procedure>
    <procedure name="Notify" visibility="public" procflags="abstract virtual">
      <parameters>
        <parameter name="A" type="Tactivation" />
      </parameters>
    </procedure>
  </interface>
</namespace>
